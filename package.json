{
  "name": "cauldron-react",
  "version": "0.0.3",
  "description": "Fully accessible react components library for Deque Cauldron",
  "main": "dist/cauldron.js",
  "scripts": {
    "build": "webpack --colors --config webpack.config.babel.js",
    "dev": "npm run build -- --watch",
    "lint": "eslint .",
    "prepack": "NODE_ENV=production webpack --config webpack.config.babel.js",
    "start": "webpack-dev-server --history-api-fallback --config webpack.config.demo.babel.js",
    "release": "standard-version",
    "test": "nyc npm run test:run && npm run lint && nsp check",
    "test:run": "tape -r babel-register \"test/src/**/*.js\" | tap-spec",
    "test:dev": "tape-watch -r babel-register \"test/src/**/*.js\" | tap-spec"
  },
  "author": "Harris Schneiderman",
  "license": "MPL-2.0",
  "dependencies": {
    "focus-trap-react": "^3.0.5",
    "keyname": "^0.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-syntax-highlighter": "^6.1.1",
    "unique-string": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dequelabs/cauldron-react.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "closest": "0.0.1",
    "css-loader": "^0.28.7",
    "deque-pattern-library": "^2.4.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.0.0-rc.14",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^7.2.2",
    "nsp": "^3.2.1",
    "nyc": "^11.3.0",
    "prettier": "1.14.2",
    "proxyquire": "^1.8.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "standard-version": "^4.3.0",
    "style-loader": "^0.19.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-watch": "^2.3.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-jarvis": "^0.1.1"
  },
  "nyc": {
    "checkCoverage": true,
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 90,
    "branches": 80,
    "functions": 80,
    "lines": 90,
    "exclude": [
      "dist",
      "coverage",
      "test/**/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
